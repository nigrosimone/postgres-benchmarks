{
  "name": "postgres-benchmarks",
  "version": "1.0.0",
  "description": "A benchmark focusing on the performance of Postgres client libraries for Node.js, brianc/node-postgres VS porsager/postgres",
  "keywords": ["nodejs", "javascript", "benchmark", "postgresql", "node-js"],
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bench": "node --expose-gc --allow-natives-syntax index.ts --iterations=50000",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mitata": "^1.0.34",
    "pg": "^8.16.3",
    "pg-native": "^3.5.2",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5"
  }
}
